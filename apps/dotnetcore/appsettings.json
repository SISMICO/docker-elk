{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:4300"
      }
    }
  },
  "ConnectionStrings": {
    "PostgreSQLDatabase": "Host=localhost; Database=postgres; Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Http"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "{ { Timestamp: @t, Level: @l, Message: @m, ServiceVersion: @p['AppVersion'], Environment: @p['EnvironmentName'], ServiceContext: @p['SourceContext'], ServiceInstance: @p['MachineName'], TransactionID: @tr, Properties: @p } }\n"
          }
        }
      },
      {
        "Name": "Http",
        "Args": {
          "requestUri": "http://localhost:8080",
          "queueLimitBytes": null,
          "formatter": {
            "type": "Serilog.Templates.ExpressionTemplate, Serilog.Expressions",
            "template": "{ { Timestamp: @t, Level: @l, Message: @m, ServiceVersion: @p['AppVersion'], Environment: @p['EnvironmentName'], ServiceContext: @p['SourceContext'], ServiceInstance: @p['MachineName'], TransactionID: @tr, Properties: @p } }\n"
          }
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ElasticApm": {
    "LogLevel": "Information",
    "ServiceName": "dotnet",
    "SecretToken": "",
    "ServerUrl": "http://localhost:8200",
    "Environment": "development",
    "TransactionSampleRate": 1.0
  }
}
