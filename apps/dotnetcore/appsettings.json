{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:4300"
      }
    }
  },
  "ConnectionStrings": {
    "PostgreSQLDatabase": "Host=localhost; Database=postgres; Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Http"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{ElasticApmTraceId} {ElasticApmTransactionId} {Message:lj} {NewLine}{Exception}"
        }
      },
      {
        "Name": "Http",
        "Args": {
          "requestUri": "http://localhost:8080",
          "queueLimitBytes": null
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId", "WithElasticApmCorrelationInfo"]
  },
  "AllowedHosts": "*",
  "ElasticApm": {
    "LogLevel": "Information",
    "ServiceName": "dotnet",
    "SecretToken": "",
    "ServerUrl": "http://localhost:8200",
    "Environment": "development",
    "TransactionSampleRate": 1.0
  }
}
